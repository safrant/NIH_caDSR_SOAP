<?xml version="1.0" encoding="UTF-8"?>

<!-- 
******************************************************************************
RFD Retrieve Form Request (ITI-34) containing an HTML based request 

FORM NAME:  NCI Demography
PURPOSE:    Requesting an NCI Demography form in SDC supported HTML format.

REVISIONS: 
Ver        Date        Author                Description
1.0        12/01/2014  Varsha Parekh /       Added RFD wrapper and schema validation
                       Richard Moldwin
                       
1.1        12/03/2014  Denise Warzel         Added formID 
1.2        12/11/2014  WS/SJ/VP/RM          InstanceID removed, schema location moved; changed formID to match SDC form_package_identifier.
******************************************************************************/
-->

<RetrieveFormRequest 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="urn:ihe:iti:rfd:2007 ../../../Schemas/RFD.xsd" 
    xmlns="urn:ihe:iti:rfd:2007"
    xmlns:sdc="http://nlm.nih.gov/sdc/form">
    
    <prepopData xsi:nil="true"/>
    
    <workflowData>
        <!-- This is a form package name, not the form identifier, requested return type - html - embedded in name-->
        <formID>
            http://nci.nih.gov/xml/owl/cadsr/form/form_design_identifier/2674812v4.0/form_package_identifier#2674812v4.0/1/html
        </formID>
        
        <encodedResponse>true</encodedResponse>
        <archiveURL/>
        <context xsi:nil="true"/>

        <!-- Comment: Optionally, insert GUID/UUID to track the RFD RetrieveFormResponse transaction -->
        <instanceID></instanceID>
        
    </workflowData>
    
</RetrieveFormRequest>
